public class NumbersAndMath { //
	public static void main( String[] args ) { //Creating Strings
		System.out.println("How To count." ); //An introduction to what the code will do
		System.out.println("30 + 30 / 2 = " + ( 30 + 30 / 2) ); //Adding and Dividing numbers
		System.out.println("100 - 99 * 100 = " + (100 - 99 * 100) ); //Subtracting and Multiplying numbers
		System.out.println("True or false: 3 + 3 < 6 - 7?"); //Asking a true or flase
		System.out.println(3 + 3 < 6 - 7); //Determining if 3+3 is less than 6-7
		System.out.println("Is 5 greater than -5? " + ( 5 > -5 )); //Determining if 5 is greater than -5
		System.out.println("Is 5 greater than or equal to -5 ? " + ( 5 >= -5 )); //Determining if 5 is greater than or equal to -5
		System.out.println("Is 5 less than or equal to -5 " + ( 5 <= -5 )); //Determining if 5 is less than or equal to -5
	} //Closing the strings
} //Closing the code
