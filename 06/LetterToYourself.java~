public class LetterToYourself {
	public static void main(String[] args) {
		System.out.println("+---------------------------------------------------------+");
		System.out.println("|                                                    #### |");
		System.out.println("|                                                    #### |");
		System.out.println("|                                                    #### |");
		System.out.println("|                                                         |");
		System.out.println("|                                                         |");
		System.out.println("|                              Jaron Bharat               |");
		System.out.println("|                              4 St Robs. CHS             |");
		System.out.println("|                              Markham, ONT 1010          |");
		System.out.println("|                                                         |");
		System.out.println("+---------------------------------------------------------+");
	}
}
